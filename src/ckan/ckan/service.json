{
    "name": "Ckan",
    "apiVersion": "2",
    "description": "CKAN is a knowledge archive",
    "operations": {
        "GetDatasets": {
            "httpMethod": "GET",
            "uri": "rest/dataset",
            "summary": "Gets a list of datasets",
            "responseClass": "GetDatasetsOutput"
        },
        "GetDataset": {
            "httpMethod": "GET",
            "uri": "rest/dataset/{id}",
            "summary": "Retrieves a dataset",
            "responseClass": "GetDatasetOutput",
            "parameters": {
                "id": {
                    "location": "uri",
                    "description": "Dataset reference to retrieve",
                    "required": true
                }
            }
        },
        "GetGroups": {
            "httpMethod": "GET",
            "uri": "rest/group",
            "summary": "Retrieves a list of groups",
            "responseClass": "GetGroupsOutput"
        },
        "GetGroup": {
            "httpMethod": "GET",
            "uri": "rest/group/{group_id}",
            "summary": "Retrieves a group",
            "responseClass": "GetGroupOutput",
            "parameters": {
                "group_id": {
                    "location": "uri",
                    "description": "The id of the group to retrieve",
                    "required": true
                }
            }
        }, 
        "GetRevisionSinceRevision": {
            "httpMethod": "GET",
            "uri": "/api/search/revision?since_id={revision_id}",
            "summary": "List revisions since revsion id.",
            "responseClass": "GetRevisionsOutput",
            "parameters": {
                "revision_id": {
                    "location": "uri",
                    "description": "The start revision you want to list revsions after",
                    "required": true
                }
            }
        }
    },
    "models": {
        "GetDatasetsOutput": {
            "type": "object",
            "properties": {
                "datasets": {
                    "description": "List of datasets as a JSON document",
                    "type": "string",
                    "location": "json"
                }
            }
        },
        "GetDatasetOutput": {
            "type": "object",
            "properties": {
                "license_title": {
                    "location": "json",
                    "type": "string"
                },
                "license": {
                    "location": "json",
                    "type": "string"
                },
                "license_id": {
                    "location": "json",
                    "type": "string"
                },
                "id": {
                    "location": "json",
                    "type": "string"
                },
                "author": {
                    "location": "json",
                    "type": "string" 
                },
                "resources": {
                    "location": "json",
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "content_length": {
                                "location": "json",
                                "type": "string"
                            },
                            "resource_group_id": {
                                "location": "json",
                                "type": "string"
                            },
                            "url": {
                                "location": "json",
                                "type": "string"
                            }

                        }
                    }
                }
            }
        },
        "GetGroupsOutput": {
            "type": "object",
            "properties": {
                "groups": {
                    "description": "List of all groups on CKAN",
                    "type": "string",
                    "location": "json"
                    
                }
            }
        },
        "GetGroupOutput": {
            "type": "object",
            "properties": {
                "group_id": {
                    "location": "json",
                    "type": "string"
                }
            }
        }
        
    }
}
